<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#690CAC"
                android:endColor="#B2A7B8"
                android:angle="50"/>
            <corners android:radius="100in"/>
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:top="2dp"
        android:left="2dp"
        android:right="2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#20222D"/>
            <corners android:radius="100in"/>
        </shape>
    </item>
</layer-list>