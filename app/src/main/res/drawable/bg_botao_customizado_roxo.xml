<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#690CAC"
                android:endColor="#690CAC"
                android:angle="100"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item
        android:bottom="1.5dp"
        android:top="1.5dp"
        android:left="1.5dp"
        android:right="1.5dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#AE56F0"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>

</layer-list>